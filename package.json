{
  "name": "star-wars-api-react",
  "version": "0.1.0",
  "description": "Search API with React and SWAPI",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pamelasantoss/star-wars-api-react.git"
  },
  "author": "Pamela Santos",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pamelasantoss/star-wars-api-react/issues"
  },
  "homepage": "https://flamboyant-meitner-87110c.netlify.com/",
  "dependencies": {
    "axios": "^0.18.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.2"
  },
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/styles/ -o src/styles/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
